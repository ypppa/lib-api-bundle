{
    "name": "paysera/lib-api-bundle",
    "version": "1.7.0",
    "description": "Symfony bundle that allows easy configuration for your REST endpoints.",
    "keywords": ["REST", "RESTful", "API"],
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Paysera\\Bundle\\ApiBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paysera\\Bundle\\ApiBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.4.34|^4.3|^5.4|^6.3",
        "symfony/security-bundle": "^3.4.34|^4.3|^5.4|^6.3",
        "symfony/validator": "^3.4.34|^4.3|^5.4|^6.3",
        "paysera/lib-normalization-bundle": "dev-SUPPORT-86537-support-symfony-6",
        "paysera/lib-normalization": "^1.2",
        "paysera/lib-object-wrapper": "~0.1",
        "paysera/lib-pagination": "dev-symfony-6.3",
        "paysera/lib-dependency-injection": "dev-SUPPORT-86538-symfony-6-support",
        "psr/log": "^1.0|^2.0",
        "doctrine/persistence": "^1.3.8 || ^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^9.0",
        "mockery/mockery": "^1.2.4",
        "symfony/yaml": "^3.4.34|^4.3|^5.4|^6.3",
        "doctrine/doctrine-bundle": "^1.12.0|^2.1",
        "doctrine/orm": "~2.5.14|^2.6.3",
        "doctrine/annotations": "^v1.14"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ypppa/lib-dependency-injection.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ypppa/lib-normalization-bundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nickbass72/lib-pagination.git"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "ocramius/package-versions": false
        }
    },
    "scripts": {
        "phpunit": "phpunit",
        "test": ["@phpunit"]
    }
}
